package game;

message qunxiongzhulu_info
{
	optional int32 season = 1[default = 0];
}

/// 去跨服 或者战斗服 注册
message req_gamesvr_register
{
    repeated int32 server_list = 1;
    optional uint32 server_open_time = 2[default = 0];
    optional int32 to_server_type = 3[default = 0];  // 0 为去跨服， 1 为去战斗服
    repeated uint32 online_uid_list = 4;  // 在线玩家
	optional qunxiongzhulu_info qunxiongzhulu_info = 5;
}

message rsp_gamesvr_register
{
    optional int32 result = 1 [default = 0];
    optional int32 to_server_type = 3[default = 0];  // 0 为去跨服， 1 为去战斗服

    optional int32 battlesvr_id = 10[default = 0];
    optional int32 battle_ver = 11[default = 0];  // 如果是战斗服 表示战斗服的version
}
